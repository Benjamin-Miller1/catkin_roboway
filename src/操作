scp /home/autolabor/.ros/test.* 192.168.8.101:/home/autolabor/

//启动键盘控制
nohup rosrun  roboway_tool keyboard_control  </dev/null >/dev/null 2>&1 &


  <node name="cartographer_node" pkg="cartographer_ros"
      type="cartographer_node" args="
          -configuration_directory $(find bringup)/param/cartographer
          -configuration_basename backpack_2d.lua
          -save_state_filename test.pbstream"
